<Page x:Class="PlexShareApp.SessionsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PlexShareApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SessionsPage">

    <Grid Background="AntiqueWhite">
        <StackPanel Margin="0,0,0,0">
            <Label HorizontalAlignment="Center" FontFamily="Times New Roman" FontSize="20" HorizontalContentAlignment="Center" Margin="0,0,0,20" Width="800" Background="Azure">Sessions Conducted</Label>
            <ScrollViewer Height="365" Margin="0,0,0,50" Padding="10">
                <StackPanel x:Name="Stack" Orientation="Vertical">
                    
                </StackPanel>
            </ScrollViewer>
        </StackPanel>
    </Grid>
</Page>
